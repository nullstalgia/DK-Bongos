{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:USERPROFILE}/Documents/Arduino/libraries/**",
                "${env:ProgramFiles(x86)}/Arduino/hardware/arduino/avr/cores/**",
                "${env:ProgramFiles(x86)}/Arduino/hardware/arduino/avr/variants/standard",
                "${env:LOCALAPPDATA}/Arduino15/**"
            ],
            "defines": [
                "USBCON"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "msvc-x64",
            "forcedInclude": [
                "${env:ProgramFiles(x86)}/Arduino/hardware/arduino/avr/cores/arduino/Arduino.h"
            ]
        }
    ],
    "version": 4
}
